@startuml
class Diver {
    - ID
    - First Name
    - Last Name
    - Date of Birth
    - Email
    - Password
    - Certifications
    - Dive Logs
    - Equipment
}

class DiveClub {
    - ID
    - Name
    - Location
    - Contact Information
    - Website
    - Dive Sites
    - Instructors
}

class DiveInstructor {
    - ID
    - Name
    - Certification Level
    - Contact Information
    - Employment History
    - Dives Led
}

class DiveSite {
    - ID
    - Name
    - Location
    - Description
    - Depth
    - Water Type
}

class DiveLog {
    - ID
    - Diver
    - Date
    - Time
    - Dive Site
    - Dive Instructor
    - Duration
    - Depth
    - Water Temperature
    - Equipment Used
    - Notes
}

class Certification {
    - ID
    - Diver
    - Name
    - Level
    - Date Received
    - Issuing Instructor
    - Dive Club
}

class Equipment {
    - ID
    - Name
    - Description
    - Quantity
}

Diver *-- DiveLog
Diver *-- Certification
Diver *-- Equipment
DiveClub *-- DiveSite
DiveClub *-- DiveInstructor
DiveInstructor *-- DiveLog
DiveLog *-- DiveSite

@enduml